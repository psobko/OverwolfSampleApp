{
    "manifest_version":"1",
    "type":"WebApp",
    "meta":{
        "name":"Sample",
        "version":"1.0",
        "minimum-overwolf-version":"0.77.0",
        "author":"Peter Sobkowski",
        "icon":"IconMouseOver.png",
        "icon_gray":"IconMouseNormal.png",
        "description":"Some basic application"
        },
    "permissions": ["GameInfo"],
    "data": {
        "start_window": "MainWindow",
        "windows": {
            "MainWindow": {
                "file": "Files/index.html",
                "transparent": true,
                "resizable": true,
                "size": {"width": 400, "height": 300},
                "min_size": {"width": 200, "height": 200},
                "max_size": {"width": 600, "height": 500}
            }
        }
    }
}